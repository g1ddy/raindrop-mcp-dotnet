{
  "servers": {
    "RaindropMcp": {
      "type": "stdio",
      "command": "dnx",
      "args": [
        "Raindrop.Mcp.DotNet",
        "--prerelease",
        "--yes"
      ],
      "env": {
        "Raindrop:ApiToken": "${env:RAINDROP_API_TOKEN}",
        "Raindrop:BaseUrl": "https://api.raindrop.io/rest/v1"
      }
    }
  }
}
